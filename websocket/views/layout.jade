doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1")
    //-  The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    meta(name="description", content="")
    meta(name="author", content="")
    link(rel="icon", href="../../favicon.ico")
    title Dashboard Template for Bootstrap
    //- Bootstrap core CSS
    link(rel='stylesheet' href='/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet' href='/bootstrap/dist/css/bootstrap-theme.min.css')
    link(rel='stylesheet' href='/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css')
    link(rel='stylesheet' href='/font-awesome/css/font-awesome.min.css')
    //- Custom CSS
    link(rel='stylesheet' href='/stylesheets/dashboard.css')
    //- jQuery
    script(type='text/javascript' src='/jquery/dist/jquery.min.js')
    //- Bootstrap Core JavaScript
    script(type='text/javascript' src='/bootstrap-switch/dist/js/bootstrap-switch.min.js')
    script(type='text/javascript' src='/bootstrap/dist/js/bootstrap.min.js')
    //- Custom JS
    script(type='text/javascript' src='/javascripts/main.js')
    block js

  body
    include header
    include slidebar
    block content


